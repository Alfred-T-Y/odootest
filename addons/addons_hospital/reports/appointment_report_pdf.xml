<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <template id="template_appointment_pdf">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <br/>
                        <br/>
                        <h1>Appointment Report</h1>
                        <br/>
                        <p class="h2"><strong>Reference : </strong><text class="text-muted" t-field="doc.reference"/></p> 
                    </div>
                </t>
            </t>
        </t>
    </template>

</odoo>
